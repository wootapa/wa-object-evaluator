{
  "name": "@wootapa/object-evaluator",
  "version": "1.0.11",
  "description": "Tests objects with logical and comparison operators using a builder pattern.",
  "main": "dist/waoe.umd.js",
  "types": "dist/waoe.d.ts",
  "module": "dist/waoe.esm.js",
  "browser": "dist/waoe.umd.js",
  "scripts": {
    "start": "npm run build",
    "lint": "eslint --fix --ext .ts ./src && eslint --fix --ext .ts ./test",
    "test": "mocha -r performance-now -r ts-node/register test/*.spec.ts",
    "build": "npm run lint && rollup -c"
  },
  "files": [
    "dist/**/*.{js,map,ts}"
  ],
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wootapa/wa-object-evaluator.git"
  },
  "keywords": [
    "WootApa",
    "Evaluate",
    "Logical",
    "Comparison"
  ],
  "author": "Andreas Petersson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wootapa/wa-object-evaluator/issues"
  },
  "homepage": "https://github.com/wootapa/wa-object-evaluator#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "eslint-config-typescript": "^3.0.0",
    "mocha": "^8.0.1",
    "rollup": "^2.10.7",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-node": "^8.10.1",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "performance-now": "^2.1.0"
  }
}
